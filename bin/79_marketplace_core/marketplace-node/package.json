{
  "name": "@rbee/marketplace-node",
  "version": "0.1.0",
  "description": "Node.js wrapper for marketplace-sdk WASM (HuggingFace, CivitAI, Worker Catalog)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "pnpm build:wasm && tsc",
    "build:wasm": "cd ../marketplace-sdk && wasm-pack build --target nodejs --out-dir ../marketplace-node/wasm",
    "dev": "tsc --watch",
    "clean": "rm -rf dist wasm"
  },
  "keywords": ["rbee", "marketplace", "huggingface", "civitai", "wasm", "node"],
  "author": "rbee Team",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@rbee/marketplace-sdk": "workspace:*",
    "@rbee/sdk-loader": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "typescript": "^5.9.3"
  }
}
